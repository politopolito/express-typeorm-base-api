{
  "name": "uncharted-be",
  "version": "1.0.0",
  "repository": "https://github.com/herrera-ignacio/uncharted-be.git",
  "author": "Ignacio Herrera <ignacioromanherrera@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-ts": "tsc",
    "build": "yarn run build-ts && yarn run lint",
    "lint": "eslint \"**/*.{js,ts}\" --fix",
    "test": "jest --forceExit --coverage --verbose",
    "dev": "yarn run build && yarn run watch-debug",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve-debug\"",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-test": "yarn run test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "concurrently": "^7.0.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "~4.5.5"
  },
  "dependencies": {
    "@types/express-serve-static-core": "^4.17.28",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^6.1.1",
    "jwks-rsa": "^2.0.5",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.0"
  }
}
